<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.zhiying.zhiyinglibrary">

    <!-- 获取运营商信息，用于支持提供运营商信息相关的接口 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <!-- 访问网络的权限，网络定位需要上网 -->
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".IApplication"
        android:allowBackup="true"
        android:icon="@mipmap/zhiying_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/zhiying_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".activity.ZYMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.ZYWebviewActivity">
        </activity>
    </application>

</manifest>